<!doctype html>
<html>

<head>
  <meta charset="utf-8" />
  <title>State Spending Vs. Graduation Rate</title>
  <meta name="author" content="Lux">
  <meta name="description" content="A look at state spending vs graduation rate data">
  <meta content="text/html" http-equiv="Content-type" />
  <meta content="width=device-width, initial-scale=1" name="viewport" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <script src="https://cdn.plot.ly/plotly-2.6.3.min.js"></script>

</head>

<body>
  <div id="tester" style="width:600px;height:250px;"></div>
  <%const spending = [];%>
  <%const rates = [];%>
  <% for(let i=1; i<data.length; i++){ %>
  <%  spending.push(data[i].spending);%>
  <%  rates.push(data[i].gradRate);%>
  <%}%>
  <%let spendingEJS = spending%>
  <% let ratesEJS = rates%>

  <%console.log(spendingEJS)%>

  <script>
  let spendJS = [];
  let spendingJS = "(<%=spendingEJS%>)";
  let spendArrayJS = [];
  spendArrayJS = spendingJS.split(",");
  console.log(spendArrayJS);
  let ratesJS = [];
  let rateJS = "<%=ratesEJS%>)";
  let rateArrayJS = rateJS.split(",");
  for(let i=0; i<spendArrayJS.length;i++){
    spendJS.push(parseInt(spendArrayJS[i]));
  }
  for(let i=0; i<rateArrayJS.length;i++){
    ratesJS.push(parseInt(rateArrayJS[i]));
  }
  console.log(spendJS);
  TESTER = document.getElementById('tester');
  var trace = {x: spendArrayJS, y: rateArrayJS, mode: "markers", type: "scatter"};
  Plotly.newPlot( TESTER,[{ x: spendArrayJS, y: rateArrayJS }] );
  </script>
  <%- include ('partials/header'); %>
  <%- include ('partials/navbar'); %>
  <header>
    <h1> US States </h1>
  </header>

  <section id="state-table">
    <table>
      <tr>
        <th>name</th>
        <th>spending</th>
        <th>gradRate</th>
      </tr>
      <% for(let i=1; i<data.length; i++){ %>
      <tr>
        <td><%= data[i].name %></td>
        <td><%= data[i].spending %></td>
        <td><%= data[i].gradRate %></td>
      </tr>
      <%}%>
    </table>
  </section>

  <%- include ('partials/footer'); %>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>
